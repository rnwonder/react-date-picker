{
  "version": "0.0.88",
  "license": "MIT",
  "description": "A responsive, highly-customizable datepicker component for ReactJS.",
  "repository": "https://github.com/rnwonder/solid-date-picker",
  "bugs": "https://github.com/rnwonder/solid-date-picker/issues",
  "homepage": "https://soliddatepicker.netlify.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "themes/**",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./dateMath": {
      "default": "./dist/dateMath.js"
    },
    "./utilities": {
      "default": "./dist/utilities.js"
    },
    "./timePicker": {
      "default": "./dist/timePicker.js"
    },
    "./monthSelector": {
      "default": "./dist/monthSelector.js"
    },
    "./calendarArea": {
      "default": "./dist/calendarArea.js"
    },
    "./popover": {
      "default": "./dist/popover.js"
    },
    "./yearSelector": {
      "default": "./dist/yearSelector.js"
    },
    "./calendar": {
      "default": "./dist/calendar.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./themes/ark-ui": {
      "default": "./themes/ark-ui.css"
    },
    "./themes/shad-cn-ui": {
      "default": "./themes/shad-cn-ui.css"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "dateMath": [
        "./dist/dateMath.d.ts"
      ],
      "utilities": [
        "./dist/utilities.d.ts"
      ],
      "timePicker": [
        "./dist/timePicker.d.ts"
      ],
      "monthSelector": [
        "./dist/monthSelector.d.ts"
      ],
      "calendarArea": [
        "./dist/calendarArea.d.ts"
      ],
      "popover": [
        "./dist/popover.d.ts"
      ],
      "yearSelector": [
        "./dist/yearSelector.d.ts"
      ],
      "calendar": [
        "./dist/calendar.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry src/**.ts",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@rnwonder/react-date-picker",
  "author": "rnwonder",
  "module": "dist/@rnwonder/react-date-picker.esm.js",
  "size-limit": [
    {
      "path": "dist/@rnwonder/react-date-picker.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/@rnwonder/react-date-picker.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "husky": "^9.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.6",
    "@rnwonder/simple-datejs": "^1.0.8",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.475.0"
  }
}
